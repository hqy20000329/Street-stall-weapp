<!--miniprogram/pages/vistor/vistor.wxml-->
<navigation-bar></navigation-bar>
<visitor-active wx:if='{{active==1}}'></visitor-active>
<visitor-user wx:if='{{active==2}}'></visitor-user>
<van-tabbar active="{{ active }}" bind:change="onChange">
  <van-tabbar-item icon="home-o">{{pageList[0].text}}</van-tabbar-item>
  <van-tabbar-item icon="search">{{pageList[1].text}}</van-tabbar-item>
  <van-tabbar-item icon="friends-o">{{pageList[2].text}}</van-tabbar-item>
</van-tabbar>
